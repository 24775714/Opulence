{
  "name": "opulence/opulence",
  "description": "The Opulence PHP framework",
  "keywords": [
    "PHP",
    "full stack",
    "template",
    "mvc",
    "database",
    "orm",
    "console",
    "apex",
    "framework"
  ],
  "authors": [
    {
      "name": "David Young",
      "email": "dave@opulencephp.com",
      "role": "Architect"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Opulence\\": [
        "src/Opulence",
        "tests/src/Opulence"
      ]
    }
  },
  "require": {
    "php": ">=7.0.0",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "jeremeamia/superclosure": "~2.1",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpredis": "2.2.7",
    "predis/predis": "1.1.*@dev",
    "phpunit/phpunit": "5.1.*",
    "phpunit/php-file-iterator": ">=1.4.1",
    "phpunit/php-token-stream": ">=1.4.8",
    "phpunit/php-text-template": ">=1.2.1",
    "phpunit/php-invoker": ">=1.1.4",
    "phpunit/phpunit-selenium": ">=2.0.0"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "phpredis",
        "version": "2.2.7",
        "source": {
          "url": "git://github.com/phpredis/phpredis.git",
          "type": "git",
          "reference": "2.2.7"
        }
      }
    }
  ],
  "replace": {
    "opulence/applications": "self.version",
    "opulence/authentication": "self.version",
    "opulence/bootstrappers": "self.version",
    "opulence/cache": "self.version",
    "opulence/console": "self.version",
    "opulence/cryptography": "self.version",
    "opulence/databases": "self.version",
    "opulence/debug": "self.version",
    "opulence/environments": "self.version",
    "opulence/events": "self.version",
    "opulence/files": "self.version",
    "opulence/framework": "self.version",
    "opulence/http": "self.version",
    "opulence/ioc": "self.version",
    "opulence/memcached": "self.version",
    "opulence/orm": "self.version",
    "opulence/pipelines": "self.version",
    "opulence/querybuilders": "self.version",
    "opulence/redis": "self.version",
    "opulence/routing": "self.version",
    "opulence/sessions": "self.version",
    "opulence/validation": "self.version",
    "opulence/views": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev"
}