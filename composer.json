{
  "name": "rdev/rdev",
  "description": "The RDev PHP framework",
  "keywords": [
    "PHP",
    "database",
    "orm",
    "framework"
  ],
  "authors": [
    {
      "name": "David Young",
      "email": "dbyoung2@gmail.com",
      "role": "Architect"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "RDev\\": [
        "app/rdev",
        "tests/app/rdev"
      ]
    }
  },
  "require": {
    "php": ">=5.5.10",
    "monolog/monolog": "1.11.*@dev"
  },
  "require-dev": {
    "alecesaro/phpredis": "dev-master",
    "predis/predis": "1.1.*@dev",
    "symfony/yaml": "2.6.*@dev",
    "phpunit/phpunit": "4.2.*",
    "phpunit/php-file-iterator": ">=1.3.0",
    "phpunit/php-token-stream": ">=1.1.3",
    "phpunit/php-text-template": ">=1.2.0",
    "phpunit/php-invoker": ">=1.1.0",
    "phpunit/phpunit-selenium": ">=1.2",
    "phpunit/phpunit-story": "*"
  },
  "replace": {
    "rdev/applications": "self.version",
    "rdev/authentication": "self.version",
    "rdev/configs": "self.version",
    "rdev/console": "self.version",
    "rdev/cryptography": "self.version",
    "rdev/databases": "self.version",
    "rdev/files": "self.version",
    "rdev/http": "self.version",
    "rdev/ioc": "self.version",
    "rdev/orm": "self.version",
    "rdev/sessions": "self.version",
    "rdev/users": "self.version",
    "rdev/views": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  },
  "minimum-stability": "dev"
}