{
  "name": "rdev/rdev",
  "description": "The RDev PHP framework",
  "keywords": [
    "PHP",
    "full stack",
    "template",
    "mvc",
    "database",
    "orm",
    "console",
    "framework"
  ],
  "authors": [
    {
      "name": "David Young",
      "email": "dbyoung2@gmail.com",
      "role": "Architect"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "RDev\\": [
        "app/rdev",
        "tests/app/rdev"
      ]
    }
  },
  "require": {
    "php": ">=5.5.10",
    "ext-openssl": "*",
    "ext-mcrypt": "*",
    "ext-mbstring": "*",
    "monolog/monolog": "1.12.*@dev",
    "symfony/security-core": "2.6.*"
  },
  "require-dev": {
    "phpredis": "2.2.7",
    "predis/predis": "1.1.*@dev",
    "phpunit/phpunit": "4.6.*",
    "phpunit/php-file-iterator": ">=1.3.4",
    "phpunit/php-token-stream": ">=1.4.0",
    "phpunit/php-text-template": ">=1.2.0",
    "phpunit/php-invoker": ">=1.1.3",
    "phpunit/phpunit-selenium": ">=1.4",
    "phpunit/phpunit-story": "*"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "phpredis",
        "version": "2.2.7",
        "source": {
          "url": "git://github.com/phpredis/phpredis.git",
          "type": "git",
          "reference": "2.2.7"
        }
      }
    }
  ],
  "replace": {
    "rdev/applications": "self.version",
    "rdev/authentication": "self.version",
    "rdev/console": "self.version",
    "rdev/cryptography": "self.version",
    "rdev/databases": "self.version",
    "rdev/files": "self.version",
    "rdev/framework": "self.version",
    "rdev/http": "self.version",
    "rdev/ioc": "self.version",
    "rdev/orm": "self.version",
    "rdev/routing": "self.version",
    "rdev/sessions": "self.version",
    "rdev/users": "self.version",
    "rdev/views": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5.x-dev"
    }
  },
  "minimum-stability": "dev"
}